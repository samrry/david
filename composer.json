{
    "name": "davidnineroc/david",
    "description": "一个快速开发API的框架",
    "keywords": ["framework", "API", "david"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "DavidNineRoc",
            "email": "1033404553@qq.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "illuminate/config": "5.5.*",
        "illuminate/container": "5.5.*",
        "illuminate/contracts": "5.5.*",
        "illuminate/database": "5.5.*",
        "nikic/fast-route": "~1.2",
        "symfony/http-kernel": "~3.3",
        "symfony/http-foundation": "~3.3",
        "vlucas/phpdotenv": "^2.5@dev"
    },
    "autoload": {
        "psr-4": {
            "David\\": "kernel/src/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "6.4"
    }
